        }

        function loadAnalytics() {
          window.dataLayer = window.dataLayer || [];

          function gtag() {
            dataLayer.push( arguments );
          };

          gtag( 'js', new Date() );
          gtag( 'config', gaId, {
            'custom_map': {
              'dimension10': 'client_id'
            },
