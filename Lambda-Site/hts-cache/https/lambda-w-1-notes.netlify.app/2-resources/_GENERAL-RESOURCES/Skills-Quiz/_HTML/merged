      <li>[ ] <code>render partial: ‘shared/menu', cached: true</code></li>
      <li>[ ] <code>render_with_cache partial: ‘shared/menu'</code></li>
      <li>[ ] <code>render partial: ‘shared/menu'</code></li>
      <li>[ ] <code>render partial: ‘shared/menu', cached_with_variables: {}</code></li>
