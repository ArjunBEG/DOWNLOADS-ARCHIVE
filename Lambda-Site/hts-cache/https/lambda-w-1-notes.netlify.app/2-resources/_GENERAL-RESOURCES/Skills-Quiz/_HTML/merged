        ON
        Students.department = Departments.department;</li>
      <li>[ ] SELECT Students.first_name, Students.last_name, Departments.office_location FROM Students JOIN
        Departments;
      </li>
      <li>[ ] SELECT Students.first_name, Students.last_name, Departments.office_location FROM Students ON
        Students.department = Departments.department;</li>
    </ul>
