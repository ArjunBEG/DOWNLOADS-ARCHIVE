<a class="sourceLine" id="cb29-2" title="2">        <span class="kw">&lt;intent-filter&gt;</span></a>
<a class="sourceLine" id="cb29-3" title="3">            <span class="kw">&lt;action</span><span class="ot"> android:name=</span><span class="st">&quot;android.intent.action.MAIN&quot;</span> <span class="kw">/&gt;</span></a>
