{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "consistent-return": 0,
    "rules": {
      "no-param-reassign": 0,
      "max-len": 0,
      "no-plusplus": 0,
      "linebreak-style": 0,
      "consistent-return": 0,
      "no-useless-return": 0,
      "no-return-assign": 0,
      "class-methods-use-this": 0,
      "no-tabs": 0
    }
}