{
	"auto_complete": true,
	"bold_folder_labels": true,
	"caret_extra_width": 2,
	"folder_exclude_patterns":
	[
		".svn",
		".git",
		".hg",
		"CVS",
		"node_modules",
		"scss",
		"zip",
		"bower_components"
	],
	"font_size": 15,
	"highlight_line": true,
	"highlight_modified_tabs": true,
